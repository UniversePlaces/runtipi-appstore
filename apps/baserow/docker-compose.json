{
  "services": [
    {
      "name": "baserow",
      "image": "baserow/baserow:1.33.2",
      "environment": {
        "BASEROW_PUBLIC_URL": "http://localhost"
      },
      "internalPort": 80,
      "addPorts": [
        {
          "containerPort": 443,
          "hostPort": 8382,
          "tcp": true,
          "udp": false
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/baserow",
          "containerPath": "/baserow/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}
